# terminal control sequences

clear_screen    \x1b[2J
home_cursor     \x1b[H
antihome_cursor \x1b[999C\x1b[999B
read_cursor_pos \x1b[6n
cursor_off      \x1b[?25l
cursor_on       \x1b[?25h
clear_row       \x1b[K
cursor_pos_fmt  \x1b[%d;%dH
colors_invert   \x1b[7m
colors_normal   \x1b[m
color_red       \x1b[31m
color_white     \x1b[39m

# other stuff

tilde           ~
rn              \r\n
space           " "

